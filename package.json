{
  "private": true,
  "name": "sijak",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "nuxi prepare",
    "dev": "pnpm db:push && nuxi dev --dotenv .env.dev",
    "lint:typecheck": "nuxi typecheck",
    "lint:eslint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint": "run-p -s lint:*",
    "lintfix": "pnpm lint:eslint --fix",
    "build": "nuxi build",
    "db:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.dev up -d",
    "db:push": "drizzle-kit push:pg --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate:pg --config drizzle.config.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.11.3",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/forms": "^0.5.7",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "class-variance-authority": "^0.7.0",
    "consola": "^3.2.3",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.10.3",
    "pinia": "^2.1.7",
    "tailwind-merge": "^2.2.1",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
